    "@nuxt/devtools": "^0.6.1",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.5.3",
    "@nuxt/test-utils": "^3.5.3",
    "@pinia/nuxt": "^0.4.11",
    "eslint": "^8.42.0",
    "nuxt": "^3.5.3"
